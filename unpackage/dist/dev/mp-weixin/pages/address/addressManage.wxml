<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['addressData']]]]]}}" value="{{addressData.username}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','telephone','$event',[]],['addressData']]]]]}}" value="{{addressData.telephone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地区</text><text data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.addressName+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" placeholder="街道门牌信息" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','detailAddress','$event',[]],['addressData']]]]]}}" value="{{addressData.detailAddress}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>